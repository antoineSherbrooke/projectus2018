.openModalAddSprint {
    background:rgb(0, 73, 37) !important;
}

.sprintState{
    display:inline-flex;
}

.aditionalButtons{
    margin-left:10px;
    margin-top:5px;
}

.modalDisableRelease img {
    float:right !important;
    transform:none !important;
    margin-top:0 !important;
}

.activateRelease img{
    float:right !important;
    transform:none !important;
    margin-top:0 !important;
}

.addRelease{
    width:60px;
    height:60px;
    border-radius:50%;
    position: fixed;
    bottom: 20px;
    right: 20px;
    font-size:60px;
}

.noFeature {
    color:red;
}

.feature-list {
    min-width:30%;
}

input.sprintName {
    width:auto;
}

.accordion div.ui-accordion-header.running {
    background :#cddc39;
}

.accordion .ui-accordion-header .italic {
    font-size:12px;
}

.table-sprint tbody tr {
    cursor:pointer;
}
.table-sprint tbody tr:hover {
    background:#b0db91;
}
