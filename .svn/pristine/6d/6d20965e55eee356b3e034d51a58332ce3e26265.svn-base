 @(release:Releases)
 
 <!-- Modal content-->
 <div class="modal-header">
 	<button type="button" class="close" data-dismiss="modal">&times;</button>
    <div class="modal-title" >
    	<h4 id="title-sprint">Disable the release : @release.getName()</h4>
    </div>
</div>
        
<div  class="modal-body" style="padding-top:0px;">
	
</div>
        

<div id="@release.getId()" class="modal-footer">
	<button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
	<button type="button" class="btn btn-primary" data-dismiss="modal" id="disableRelease">Disable</button>
</div>



