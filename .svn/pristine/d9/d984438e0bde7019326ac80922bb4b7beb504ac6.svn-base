    font-stretch: condensed;  
}

.userTeam{
    font-size: 1.5em;
    font-family: sans-serif;
    text-decoration: none;
    color:#3a3a3a;
    margin-right: 2em;
}
.userNotTeam{
    font-size: 1.5em;
    font-family: sans-serif;
    text-decoration: none;
    color:#3a3a3a;
    margin-right: 2em;
    width: 8em;
}

.notMembers{
    display: flex;
    flex-direction: row;
    align-items: center;
    align-content: center;
    
}
/*----icon from cssicon.space----*/

.minus_icon { 
    color: #000; 
    position:absolute; 
    width: 1em; 
    height: 1px;
    border-radius: 25%;
    background-color: #3a3a3a;
}

.trash_icon {
  color: #000;
  position: absolute;
  width: 9px;
  height: 10px;
  border-left: solid 1px #3a3a3a;
  border-right: solid 1px #3a3a3a;
  border-bottom: solid 1px #3a3a3a;
  border-radius: 0 0 2px 2px;
    
}

.trash_icon:before {
  content: '';
  position: absolute;
  left: -4px;
  top: -2px;
  width: 17px;
  height: 1px;
  background-color: #3a3a3a;
}

.trash_icon:after {
    content: '';
    position: absolute;
    left: 0px;
    top: -5px;
    width: 7px;
    height: 2px;
    border-left: solid 1px #3a3a3a;
    border-right: solid 1px #3a3a3a;
    border-top: solid 1px #3a3a3a;
    border-radius: 4px 4px 0 0;
    border-bottom-color:#3a3a3a;
}   


.plus_icon {
    color: #000;
    position: absolute;

}

.plus_icon:before {
    content: '';
    position: absolute;
    width: 15px;
    height: 1px;
    margin-left: -0.5em;
    background-color: #3a3a3a;
}

.plus_icon:after {
    content: '';
    position: absolute;
    width: 15px;
    height: 1px;
    margin-left: -0.5em;
    background-color: #3a3a3a;
    -webkit-transform: rotate(90deg);
          transform: rotate(90deg);
    }






