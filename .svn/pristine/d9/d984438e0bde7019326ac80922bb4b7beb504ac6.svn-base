*{
    margin: 0px;
    padding: 0px;
}


/*-----Header----*/
header
{
    background-color: #3a3a3a;
}

#imgbanner
{
    width: 100%;
    height: auto;
    margin-bottom: 0px;
}

.link
{
    margin-top: 0px;
    display : flex;
    flex-direction: row;
    justify-content: space-between;
    background-color: #3a3a3a;
    padding: 2em 1em;
}

.link a
{
    color: #fff;
    text-align: center;
    padding: 2em 1em;
    text-decoration: none;
    font-weight: 700;
    font-family: 'Arial', sans-serif;
}

.link a:hover
{
    background-color: #ddd;
    color: #00a758;
    transition: color 1s, transform 2s;
    transition: background 1s, transform 2s;
}


/*-----Messages----*/


@keyframes transition
{
0%   { margin-left: -10px;opacity:0; }
100% { margin-left: 0;opacity:1; }
}

.messages
{
    height: 5em;
    margin: 0em;
    opacity:0;animation: transition 0.2s ease-in-out 0.5s forwards;
}

.texte
{
    font-family: 'Arial', sans-serif;
    padding: 1em 2em;
    font-weight: bold;
    color: #FFF;
}

.error
{
    background-color: #FB0000;
}

.success
{
    background-color: #2d9d5f;
}

.role
{
    background-color: #3a3a3a;
}


/*-----Content----*/


