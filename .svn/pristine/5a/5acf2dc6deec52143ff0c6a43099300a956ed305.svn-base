@* ContentEditHome Template File *@
@(project : Project)
@defining(play.core.PlayVersion.current) { version =>

	
	@helper.form(routes.HomeController.postEditHome()) {
	@helper.CSRF.formField
	
	<h1 id="ledithomepage"></h1>
	<h2>@project.getName()</h2>
	<div class="textAreaHome">
		<div>
		    <h2 id="lresumeen"></h2>
			<textarea type="text" name="resumeEn" id="resume" cols="80" rows="20">@project.getResumeEn()</textarea>
		</div>
		
		<div style="margin-left:1em;">
			<h2 id="lresumefr"></h2>
			<textarea type="text" name="resumeFr" id="resume" cols="80" rows="20">@project.getResumeFr()</textarea>
		</div>
	</div>
	<button type="submit" id="bvalidate" name="send" value="send"></button>
	<button type="submit" id="bcancel" name="send" value="cancel"></button>	
	}

}